#!/bin/bash
S_PATH=$HOME/Kelp/src/
echo $S_PATH

cd $S_PATH/comm && make && cd $S_PATH/KNN/ && make && cd $S_PATH/KCN/ && make && cd $S_PATH/KSN/ && make && sudo ldconfig && echo "done!"
